{
  "name": "swagger-md",
  "description": "Converts Swagger API spec to Markdown file",
  "main": "lib/swagger-converter.js",
  "private": false,
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --cache .",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test istanbul cover _mocha",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/springworks/swagger-md.git"
  },
  "keywords": [
    "swagger",
    "markdown",
    "converter",
    "api"
  ],
  "author": "Springworks (http://springworks.se)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/springworks/swagger-md/issues"
  },
  "homepage": "https://github.com/springworks/swagger-md#readme",
  "dependencies": {
    "toc-md": "0.2.0"
  },
  "devDependencies": {
    "@springworks/test-harness": "1.3.1",
    "babel-cli": "6.8.0",
    "babel-eslint": "4.1.7",
    "babel-preset-es2015-node4": "2.1.0",
    "babel-register": "6.8.0",
    "eslint": "1.10.3",
    "eslint-config-springworks": "6.0.0",
    "eslint-plugin-import": "0.12.1",
    "eslint-plugin-mocha": "1.1.0",
    "eslint-plugin-should-promised": "1.0.6",
    "eslint-plugin-springworks": "1.1.1",
    "fixture-loader": "0.2.5",
    "istanbul": "0.4.2",
    "mocha": "2.3.4",
    "semantic-release": "4.3.5"
  }
}
